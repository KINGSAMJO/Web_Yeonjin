<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="1dp"
        android:bottom="1dp"
        android:start="1dp"
        android:end="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="3dp"
                android:color="#A4B2DF"/>
            <solid android:color="#FFF"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>
